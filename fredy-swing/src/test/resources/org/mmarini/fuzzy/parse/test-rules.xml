<?xml version="1.0" encoding="UTF-8"?>
<rules xmlns="http://www.mmarini.org/fuzzy" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.mmarini.org/fuzzy ../../../../../../main/resources/rules.xsd ">
	<rule>
		<if>
			<very>
				<predicate>P1</predicate>
			</very>
		</if>
		<then />
		<else />
	</rule>

	<rule>
		<if>
			<somewhat>
				<predicate>P1</predicate>
			</somewhat>
		</if>
		<then />
		<else />
	</rule>

	<rule>
		<if>
			<known>
				<predicate>P1</predicate>
			</known>
		</if>
		<then />
		<else />
	</rule>

	<rule>
		<if>
			<not>
				<predicate>P1</predicate>
			</not>
		</if>
		<then />
		<else />
	</rule>

	<rule>
		<if>
			<ifonlyif>
				<predicate>P1</predicate>
				<predicate>P2</predicate>
			</ifonlyif>
		</if>
		<then />
		<else />
	</rule>

	<rule>
		<if>
			<implies>
				<predicate>P1</predicate>
				<predicate>P2</predicate>
			</implies>
		</if>
		<then />
		<else />
	</rule>

	<rule>
		<if>
			<and>
				<predicate>P1</predicate>
				<predicate>P2</predicate>
				<predicate>P3</predicate>
			</and>
		</if>
		<then />
		<else />
	</rule>

	<rule>
		<if>
			<or>
				<predicate>P1</predicate>
				<predicate>P2</predicate>
				<predicate>P3</predicate>
			</or>
		</if>
		<then />
		<else />
	</rule>

	<rule>
		<if>
			<predicate>P1</predicate>
		</if>
		<then>
			<assign-not>H1</assign-not>
			<assign>H2</assign>
		</then>
		<else>
			<assign-not>H3</assign-not>
			<assign>E1</assign>
		</else>
	</rule>

	<rule>
		<if>
			<false />
		</if>
		<then />
		<else />
	</rule>
	<rule>
		<if>
			<quite-false />
		</if>
		<then />
		<else />
	</rule>
	<rule>
		<if>
			<unknown />
		</if>
		<then />
		<else />
	</rule>
	<rule>
		<if>
			<quite-true />
		</if>
		<then />
		<else />
	</rule>
	<rule>
		<if>
			<true />
		</if>
		<then />
		<else />
	</rule>
	<rule>
		<if>
			<true confidence="0.8" />
		</if>
		<then />
		<else />
	</rule>
</rules>